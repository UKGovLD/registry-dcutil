{       
    "name" : "labeled-skos",
    "description" : "SKOS template which uses concept labels as the URI segement",
    "type" : "Composite",
    "oneOffs" : {
        "@id" : "{$dataset}",
        "<rdf:type>" : [ "<reg:Register>", "<skos:ConceptScheme>"],
        "<reg:inverseMembershipPredicate>" : "<skos:inScheme>"
    },
    "templates" : {
        "bind" : { "notation" : "{label.toSegment()}"},
        "template" : {
            "@id" : "<{$base}/{notation}>",
            "<rdf:type>" : "<skos:Concept>",
            "<skos:notation>" : "{notation.toString()}",
            "<dct:description>" : "{description}",
            "<skos:prefLabel>" : "{label}",
            "<rdfs:label>" : "{label}",
            "<skos:inScheme>" : "{$dataset}"
        }
    }
}